{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
      "include": ["src"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./api/",
    "recurse": true
  }
}
